\ Kevo examples\ (c) Antero Taivalsaari 1991-1992\ Classic concurrent sender/receiver program" ok.kevo" fromApplications CDDEFINED SendReceive IF endFrom THENMKDIR SendReceiveSendReceive ADDS	\ Communications queue	REF sq	Prototypes.Concurrency.SharedQueue.new -> sq	20 sq.setLimit	: giveRandom	(  -- randomNumber )		1 100 rnd ;	: sendJob		10 TIMES 		  20 TIMES giveRandom ." Sending " dup . cr		    sq.enqueue		  LOOP		  600 msecs ( six second pause ) 		LOOP stop ;	: receiveJob		BEGIN		  20 msecs	( 20 millisecond pause )		  sq.dequeue . ." received" cr		AGAIN ;	REF senderTask	Prototypes.Concurrency.TerminalTask.new -> senderTask	REF receiverTask	Prototypes.Concurrency.TerminalTask.new -> receiverTask	" Sender"	senderTask.output.setTitle	" Receiver" receiverTask.output.setTitle	' sendJob senderTask.does 	senderTask.activate	' receiveJob receiverTask.does	receiverTask.activateENDADDS;